{
  "name": "js-cloudimage-smart-crop-preview",
  "version": "1.0.0",
  "description": "Interactive focal-point-based image crop preview for multiple aspect ratios",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs"
    },
    "./css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite-demo.config.ts",
    "build": "vite build && vite build --config vite-react.config.ts",
    "build:lib": "vite build",
    "build:react": "vite build --config vite-react.config.ts",
    "build:demo": "vite build --config vite-demo.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ react/ tests/",
    "format": "prettier --write \"src/**/*.ts\" \"react/**/*.tsx\" \"tests/**/*.ts\"",
    "preview": "vite preview --config vite-demo.config.ts"
  },
  "keywords": [
    "image",
    "crop",
    "preview",
    "focal-point",
    "aspect-ratio",
    "responsive",
    "cloudimage",
    "scaleflex"
  ],
  "author": "Scaleflex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleflex/js-cloudimage-smart-crop-preview"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.0.0",
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "typescript-eslint": "^8.0.0",
    "prettier": "^3.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0"
  }
}
